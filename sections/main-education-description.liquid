<article class="article-template">
{% include 'breadcrumbs' %}
<header class="page-width page-width--narrow" {{ block.shopify_attributes }}>
          {% for block in section.blocks %}
          {% assign getblog = block.settings.arti_blg %}
          {% assign artttl = article.title %}
          {% if getblog.title == artttl %}
          {% if block.settings.ifram_ttl != blank %}
          <h2 class="article-template__title center" itemprop="headline">
               {{ block.settings.ifram_ttl }}
          </h2>
          {% endif %}
          <div class="article-template__excerpt">
            {% if block.settings.ifram_ttl2 != blank %}
            <div class="excerpt">
              {{block.settings.ifram_ttl2}}
            </div>
            {% endif %}
            {% if block.settings.ifram_id != blank %}
            <div class="large-hide mobile-vide-sec">
                <iframe width="100%" height="500" src="{{ block.settings.ifram_id }}" title="John Riley: The Art of Bop Drumming, Series Introduction" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen="" class="small-hide"></iframe>
            </div>
            {% endif %}
            <div class="narrow">
              <a href="{{blog.previous_article.url}}">
                <svg width="44" height="44" viewBox="0 0 44 44" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <circle cx="22" cy="22" r="21.5" stroke="#5C5D60"/>
                  <path d="M26 14L18 22.5L26 31" stroke="#5C5D60" stroke-width="2" stroke-linecap="round"/>
                </svg>                  
              </a>
              <span class="h5 uppercase">Lesson 2/{{blog.articles.size}} </span>
              <a href="{{blog.next_article.url}}">
                <svg width="44" height="44" viewBox="0 0 44 44" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <circle cx="22" cy="22" r="21.5" transform="rotate(-180 22 22)" stroke="#5C5D60"/>
                  <path d="M18 30L26 21.5L18 13" stroke="#5C5D60" stroke-width="2" stroke-linecap="round"/>
                </svg>                                 
              </a>
            </div>
          </div>
          {% endif %}
          {% endfor %}
</header>
<div class="article-template__content page-width page-width--narrow rte">
{% for block in section.blocks %}
{% assign getblog = block.settings.arti_blg %}
{% assign artttl = article.title %}
{% if getblog.title == artttl %}
<iframe width="100%" height="600" src="{{ block.settings.ifram_id }}" title="John Riley: The Art of Bop Drumming, Series Introduction" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen="" class="small-hide"></iframe>
<span class="small-hide">
  {{ block.settings.iframe_cap }}
</span>
{{ block.settings.desc_txt }}
<ul class="content">
              <li>
                  <iframe 
                    width="1220" 
                    height="686" 
                    src="{{ block.settings.ifram_id2}}" 
                    title="{{article.title}}" 
                    frameborder="0" 
                    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                    allowfullscreen=""
                  ></iframe> 
                  <iframe 
                    width="1220" 
                    height="686" 
                    src="{{ block.settings.ifram_id3}}" 
                    title="{{article.title}}" 
                    frameborder="0" 
                    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                    allowfullscreen=""
                  ></iframe> 
              </li> 
              <li>
                  <div class="text">
                       {{ block.settings.quote }}
                  </div>
                  <iframe 
                    width="1220" 
                    height="686" 
                    src="{{ block.settings.ifram_id4}}" 
                    title="{{article.title}}" 
                    frameborder="0" 
                    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                    allowfullscreen=""
                  ></iframe> 
              </li> 
              <li>
                  <iframe 
                    width="1220" 
                    height="686" 
                    src="{{ block.settings.ifram_id5}}" 
                    title="{{article.title}}" 
                    frameborder="0" 
                    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                    allowfullscreen=""
                  ></iframe> 
                <div class="text">
                     {{ block.settings.quote2 }}
                </div>
              </li> 
          </ul>
          {% if block.settings.quote3 != blank %}
          <div class="quote">
            {{block.settings.quote3}}
          </div>
          {% endif %}
  {% endif %}
{% endfor %}
</div>
<div class="about-artist-section page-width--narrow page-width">
  <div class="grid">
    {% for block in section.blocks %}
{% assign getblog = block.settings.arti_blg %}
{% assign artttl = article.title %}
      {% if getblog.title == artttl %}
    <div class="title">
      <h2 class="h1">{{block.settings.Artists_Title}}'s IN <span>FEW</span></h2>
    </div>
    <div class="image">
      <div class="media" style="padding-bottom: 130%;">
        <img src="{{block.settings.artist_img | img_url: 'master'}}" loading="lazy" width="356" height="485" >
      </div>
    </div>
    <div class="content">
      <div class="info">
        <ul>
          {{ block.settings.quest }}
          {{ block.settings.quest2 }}
          {{ block.settings.quest3 }}
        </ul>
      </div>
    </div>
        {% endif %}
      {% endfor %}
  </div>
</div>
<div class="image-with-text image-with-text--no-overlap   isolate collapse-corners section-543c39e1-2e95-4a79-9cb1-c4edf029d1f7-padding">
<div class="image-with-text__grid grid grid--gapless grid--1-col width--medium  ">
    <div class="image-with-text__media-item  gradient color-background-2 image-with-text__media-item--medium image-with-text__media-item--top grid__item">
      <div class="image-with-text__media image-with-text__media--adapt global-media-settings media" style="padding-bottom: 55%;">
        {% for block in section.blocks %}
{% assign getblog = block.settings.arti_blg %}
{% assign artttl = article.title %}
          {% if getblog.title == artttl %}
        <img src="{{ block.settings.get_imm | img_url: 'master'}}" loading="lazy">
            {% endif %}
          {% endfor %}
      </div>
    </div>
    <div class="image-with-text__text-item grid__item">
      <div id="ImageWithText--543c39e1-2e95-4a79-9cb1-c4edf029d1f7" class="image-with-text__content horizontal-align-content--middle image-with-text__content--top image-with-text__content--desktop-left image-with-text__content--mobile-left image-with-text__content--adapt gradient color-background-2 content-container">
        <div class="info" style="max-width: 450px;">
          <div class="text">
            {% for block in section.blocks %}
{% assign getblog = block.settings.arti_blg %}
{% assign artttl = article.title %}
              {% if getblog.title == artttl %}
            <h2 class="image-with-text__heading h2">
              {{ block.settings.get_imm2 }}
            </h2>
            <div class="image-with-text__text rte ">
              <div class="content">
                {{ block.settings.get_imm3 }}
              </div>
              
            </div>
                {% endif %}
            {% endfor %}
          </div>
        </div>
      </div>
    </div>
</div>
</div>
</article>
<style>
  .section-543c39e1-2e95-4a79-9cb1-c4edf029d1f7-padding{
    padding-top: 100px;
    padding-bottom: 36px;
  }.isolate {
    position: relative;
    z-index: 0;
}.image-with-text .image-with-text__grid.width--medium .image-with-text__media-item, .image-with-text .image-with-text__grid.width--medium .image-with-text__text-item {
    width: 50%;
}.image-with-text__content {
    padding: 6rem 5rem 7rem;
}.image-with-text .image-with-text__grid .image-with-text__text-item .image-with-text__content.horizontal-align-content--middle .info {
    margin: auto;
}
@media only screen and (max-width: 767px) {
.image-with-text .image-with-text__grid.width--medium .image-with-text__media-item, .image-with-text .image-with-text__grid.width--medium .image-with-text__text-item {
    width: 100%;
}
}
</style>
