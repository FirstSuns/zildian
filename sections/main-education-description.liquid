<article class="article-template">
<div class="article-template__content page-width page-width--narrow rte">
{% for block in section.blocks %}
{% assign getblog = block.settings.arti_blg %}
{% assign artttl = article.title %}
{% if getblog.title == artttl %}
<iframe width="100%" height="600" src="{{ block.settings.ifram_id }}" title="John Riley: The Art of Bop Drumming, Series Introduction" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen="" class="small-hide"></iframe>
<span class="small-hide">
  {{ block.settings.iframe_cap }}
</span>
{{ block.settings.desc_txt }}
<ul class="content">
              <li>
                  <iframe 
                    width="1220" 
                    height="686" 
                    src="{{ block.settings.ifram_id2}}" 
                    title="{{article.title}}" 
                    frameborder="0" 
                    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                    allowfullscreen=""
                  ></iframe> 
                  <iframe 
                    width="1220" 
                    height="686" 
                    src="{{ block.settings.ifram_id3}}" 
                    title="{{article.title}}" 
                    frameborder="0" 
                    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                    allowfullscreen=""
                  ></iframe> 
              </li> 
              <li>
                  <div class="text">
                       {{ block.settings.quote }}
                  </div>
                  <iframe 
                    width="1220" 
                    height="686" 
                    src="{{ block.settings.ifram_id4}}" 
                    title="{{article.title}}" 
                    frameborder="0" 
                    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                    allowfullscreen=""
                  ></iframe> 
              </li> 
              <li>
                  <iframe 
                    width="1220" 
                    height="686" 
                    src="{{ block.settings.ifram_id5}}" 
                    title="{{article.title}}" 
                    frameborder="0" 
                    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                    allowfullscreen=""
                  ></iframe> 
                <div class="text">
                     {{ block.settings.quote2 }}
                </div>
              </li> 
          </ul>
          {% if block.settings.quote3 != blank %}
          <div class="quote">
            {{block.settings.quote3}}
          </div>
          {% endif %}
  {% endif %}
{% endfor %}
</div>
</article>
{% schema %}
  {
    "name": "Education Decription",
    "blocks": [
        {
          "type": "text",
          "name": "Description Portion",
          "settings":[
            {
              "type": "article",
              "id": "arti_blg",
              "label": "Select Article"
            },
            {
              "type": "header",
              "content": "Article Description"
            },
            {
               "type": "text",
               "id": "ifram_id",
               "label": "Youtube Link"
            },
            {
               "type": "text",
               "id": "iframe_cap",
               "label":"Video Caption"
            },
            {
              "type": "richtext",
              "id": "desc_txt",
              "label": "Decription Content"
            },
            {
              "type": "header",
              "content": "Video With Text"
            },
            {
               "type": "text",
               "id": "ifram_id2",
               "label": "Youtube Link1"
            },
            {
               "type": "text",
               "id": "ifram_id3",
               "label": "Youtube Link2"
            },
            {
              "type": "richtext",
              "id": "quote",
              "label": "Content"
            },
            {
               "type": "text",
               "id": "ifram_id4",
               "label": "Youtube Link3"
            },
            {
               "type": "text",
               "id": "ifram_id5",
               "label": "Youtube Link4"
            },
            {
              "type": "richtext",
              "id": "quote2",
              "label": "Content"
            },
            {
              "type": "header",
              "content": "Quote"
            },
            {
              "type": "richtext",
              "id": "quote3",
              "label": "Quote Content"
            }
          ] 
        }
    ],
    "presets":[
      {
        "name": "Main Education Article"
      }
    ]
  }
{% endschema %}